{
  "schemaVersion": 1,
  "id": "powersmod",
  "version": "${version}",

  "name": "Powers Mod",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "E_M_t_O_o_C",
    "Gerg_Ops"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT License",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.fabricmc.example.ExampleMod"
    ],
    "client": [
      "net.fabricmc.example.PowersModClient"
    ]
  },
  "mixins": [
    "powersmod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
